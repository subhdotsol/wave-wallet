{
  "expo": {
    "name": "wave",
    "slug": "wave",
    "scheme": "wave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wave-logo.jpg",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/wave-logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2458f3"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#0a0a14"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wave-logo.jpg",
        "backgroundColor": "#2458f3"
      },
      "backgroundColor": "#0a0a14",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.subhdotsol.wave"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
